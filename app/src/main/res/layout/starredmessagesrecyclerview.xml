<?xml version="1.0" encoding="utf-8"?>

<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/StarredMessagesConstraintLayout"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginBottom="7dp"
    android:background="#F1F1F1"
    android:clickable="true"
    android:gravity="center"
    android:orientation="vertical"
    android:paddingLeft="16dp"
    android:paddingTop="-20dp"
    android:paddingRight="16dp"
    android:paddingBottom="-10dp">


    <LinearLayout
        android:id="@+id/StarredMessagesLinearLayout1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/StarredMessagesLinearLayout2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:layout_marginBottom="8dp"
            android:orientation="horizontal"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0">

        <ImageView
                android:id="@+id/StarredMessagesProfilePic"
            android:layout_width="49dp"
            android:layout_height="49dp"
            android:layout_marginTop="1dp"
            android:layout_marginBottom="8dp"
            android:background="@drawable/roundprofilepiccontact"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.005"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0" />

            <LinearLayout
                android:layout_width="match_parent"
                app:layout_constraintTop_toTopOf="parent"
                android:layout_marginStart="60dp"
                android:orientation="horizontal"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@id/StarredMessagesProfilePic"
                android:layout_height="20dp">

                <TextView
                    android:id="@+id/StarredMessagesMessageSender"
                    android:layout_width="wrap_content"
                    android:layout_height="18dp"
                    android:ellipsize="end"
                    android:fontFamily="@font/chivoregular"
                    android:layout_weight="1"
                    android:gravity="start"
                    android:maxLines="1"
                    android:singleLine="true"
                    android:text="Shhhhhhhhh"
                    android:textColor="#000000"
                    android:textSize="16sp" />

            <ImageView
                android:id="@+id/imageViewArrowStarredMessages"
                android:layout_width="25dp"
                android:layout_height="20dp"
                android:layout_alignParentTop="true"
                android:layout_alignRight="@+id/StarredMessagesMessageSender"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/rightarrowicon"/>


                <TextView
                    android:id="@+id/StarredMessagesMessageReceiver"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="30dp"
                    android:layout_weight="1"
                    android:ellipsize="end"
                    android:fontFamily="@font/chivoregular"
                    android:maxLines="1"
                    android:singleLine="true"
                    android:text="Rejjjjjjjjjjjjjjjjjjjjjjjjjjjjjj"
                    android:textColor="#000000"
                    android:textSize="16sp" />






                <ImageView
                    android:id="@+id/StarredMessagesForwardimageView"
                    android:layout_width="10dp"
                    android:layout_height="18dp"
                    android:layout_marginStart="5dp"
                    android:layout_marginTop="32dp"
                    android:layout_marginEnd="10dp"
                    android:layout_weight="1"
                    app:srcCompat="@drawable/forwardarrowicon" />
            </LinearLayout>


        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/StarredMessagesLinearLayout4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp"
            android:layout_marginBottom="10dp"
            android:background="@drawable/chatlistbackground"
            android:orientation="vertical"
            android:paddingEnd="0dp">

            <TextView
                android:id="@+id/StarredMessagesMessagetextView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_marginStart="10dp"
                android:layout_marginTop="10dp"
                android:layout_marginEnd="10dp"
                android:layout_marginBottom="10dp"
                android:ellipsize="end"
                android:fontFamily="@font/chivoregular"
                android:text="Multiple line messages here! Multiple line messages here!Multiple line messages here!Multiple line messages here!Multiple line messages here!Multiple line messages here!Multiple line messages here!Multiple line messages here!Multiple line messages here!Multiple line messages here!Multiple line messages here!Multiple line messages here! "
                android:textColor="#000000"
                android:textSize="17sp"
                app:layout_constraintBottom_toTopOf="@id/StarredMessagesLinearLayout5"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <LinearLayout
                android:id="@+id/StarredMessagesLinearLayout5"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:orientation="horizontal"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@id/StarredMessagesMessagetextView">

                <TextView
                    android:id="@+id/textView9"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="6dp"
                    android:layout_marginEnd="2dp"
                    android:layout_weight="0.2"
                    android:fontFamily="@font/chivoregular"
                    android:layout_marginStart="10dp"
                    android:layout_marginLeft="10dp"
                    android:text="more..."
                    android:textColor="#C1A0EE"
                    android:textSize="12dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <ImageView
                    android:id="@+id/StarredMessagesStarimageView"
                    android:layout_width="19dp"
                    android:layout_height="19dp"
                    android:layout_marginEnd="10dp"
                    android:layout_marginBottom="10dp"
                    android:layout_marginTop="6dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toStartOf="@id/StarredMessagesTimetextView"
                    app:srcCompat="@drawable/star" />


                <TextView
                    android:id="@+id/StarredMessagesTimetextView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="15dp"
                    android:fontFamily="@font/chivoregular"
                    android:text="Time"
                    android:layout_marginTop="6dp"
                    android:textColor="#000000"
                    android:textSize="13sp"/>



            </LinearLayout>
        </androidx.constraintlayout.widget.ConstraintLayout>

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>
